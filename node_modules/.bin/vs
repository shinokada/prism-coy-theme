#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+cli@4.3.2/node_modules/@vivliostyle/cli/dist/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+cli@4.3.2/node_modules/@vivliostyle/cli/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+cli@4.3.2/node_modules/@vivliostyle/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+cli@4.3.2/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+cli@4.3.2/node_modules/@vivliostyle/cli/dist/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+cli@4.3.2/node_modules/@vivliostyle/cli/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+cli@4.3.2/node_modules/@vivliostyle/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+cli@4.3.2/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vivliostyle/cli/dist/cli.js" "$@"
else
  exec node  "$basedir/../@vivliostyle/cli/dist/cli.js" "$@"
fi
