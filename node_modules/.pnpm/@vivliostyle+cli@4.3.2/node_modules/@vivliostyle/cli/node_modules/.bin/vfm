#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+vfm@1.0.2/node_modules/@vivliostyle/vfm/lib/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+vfm@1.0.2/node_modules/@vivliostyle/vfm/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+vfm@1.0.2/node_modules/@vivliostyle/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+vfm@1.0.2/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+vfm@1.0.2/node_modules/@vivliostyle/vfm/lib/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+vfm@1.0.2/node_modules/@vivliostyle/vfm/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+vfm@1.0.2/node_modules/@vivliostyle/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/@vivliostyle+vfm@1.0.2/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@vivliostyle+vfm@1.0.2/node_modules/@vivliostyle/vfm/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../@vivliostyle+vfm@1.0.2/node_modules/@vivliostyle/vfm/lib/cli.js" "$@"
fi
