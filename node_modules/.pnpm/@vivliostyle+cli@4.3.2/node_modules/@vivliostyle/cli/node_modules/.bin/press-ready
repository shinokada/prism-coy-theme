#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/press-ready@4.0.3/node_modules/press-ready/lib/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/press-ready@4.0.3/node_modules/press-ready/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/press-ready@4.0.3/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/press-ready@4.0.3/node_modules/press-ready/lib/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/press-ready@4.0.3/node_modules/press-ready/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/press-ready@4.0.3/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../press-ready@4.0.3/node_modules/press-ready/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../press-ready@4.0.3/node_modules/press-ready/lib/cli.js" "$@"
fi
