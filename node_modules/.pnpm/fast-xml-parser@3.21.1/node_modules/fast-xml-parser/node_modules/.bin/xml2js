#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/fast-xml-parser@3.21.1/node_modules/fast-xml-parser/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/fast-xml-parser@3.21.1/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/fast-xml-parser@3.21.1/node_modules/fast-xml-parser/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/fast-xml-parser@3.21.1/node_modules:/Users/shinichiokada/Vivliostyle/prism-coy-theme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
